const users = [{ name: 'Коля', age: 9 }, { name: 'Вася', age: 300 }, { name: 'Петя', age: 5 }, { name: 'Оля', age: 50 }];
for (let user of users) {
    if (user.age < 10) {
        console.log(user.name);

    }
}


function max(arr) {
    let max = arr[0];
    for(let i=1; i<arr.length; i++){
      if(arr[i] > max){
        max = arr[i];   
      }
     }
   return max;
   }
   max([5,2,7]);


const words = ['name', 'age', 'forname'];
const lengths = words.map(function(word) {
    return word + ':' + word.length;

});
console.log(lengths);

